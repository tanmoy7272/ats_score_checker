{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-railway-backend.up.railway.app/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://your-railway-backend.up.railway.app/health"
    }
  ]
}
